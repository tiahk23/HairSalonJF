@using HairSalonJF.ViewModels;
@model AddClientIntakeFormViewModel;

<h1>Client Intake Form </h1>

<form method="post">
    <div asp-controller="ClientIntakeForm" asp-action="Add" class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" />
        <span asp-validation-for="Name"></span>
    </div>
    <div class="form-group">
        <label asp-for="Age"></label>
        <input asp-for="Age" />
        <span asp-validation-for="Age"></span>
    </div>
    <div class="form-group">
        <label asp-for="Service"></label>
        <input asp-for="Service" />
        <span asp-validation-for="Service"></span>
    </div>
    <input type="submit" value="Add ClientIntakeForm" /> 
</form>